const fs = require('fs')

const content = new Date().toString().replace(/T/, ':').replace(/\.\w*/, '')+': WebExec success';

const extensionDir = __dirname;

fs.writeFile('C:\\Users\User\\Desktop\\poc.txt', content, err => {
  if (err) {
    console.error(err)
    return
  }
  //file written successfully
})
